{
  "name": "bulk-auction-factory",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "node": "npx hardhat node --no-deploy",
    "node:anvil": "dotenv -- bash -c 'anvil --port 8547 --chain-id 31338 --fork-url $ETHEREUM_SEPOLIA_RPC_URL'",
    "node:l2": "dotenv -- bash -c 'npx hardhat node --no-deploy --hostname 0.0.0.0 --fork $ARBITRUM_SEPOLIA_RPC_URL --port 8546'",
    "node:l2:anvil": "dotenv -- bash -c 'anvil --port 8546 --host 0.0.0.0 --chain-id 31338 --fork-url $ARBITRUM_SEPOLIA_RPC_URL'",
    "deploy:v1:localhost": "npx hardhat deploy --tags V1 --network localhost",
    "deploy:v1:localhost_l2": "npx hardhat deploy --tags V1 --network localhost_l2",
    "deploy:v1:base_sepolia": "npx hardhat deploy --tags V1 --network base_sepolia",
    "deploy:v1:sepolia": "npx hardhat deploy --tags V1 --network sepolia",
    "deploy:v1:holesky": "npx hardhat deploy --tags V1 --network holesky",
    "deploy:v1:mainnet": "npx hardhat deploy --tags V1 --network mainnet",
    "deploy:v1:base_mainnet": "npx hardhat deploy --tags V1 --network base_mainnet",
    "verify:v1:base_sepolia": "npx hardhat run ./src/verifyV1.ts --network base_sepolia",
    "verify:v1:sepolia": "npx hardhat run ./src/verifyV1.ts --network sepolia",
    "verify:v1:holesky": "npx hardhat run ./src/verifyV1.ts --network holesky",
    "verify:v1:mainnet": "npx hardhat run ./src/verifyV1.ts --network mainnet",
    "verify:v1:base_mainnet": "npx hardhat run ./src/verifyV1.ts --network base_mainnet",
    "deploy:v1.5:localhost": "npx hardhat deploy --tags V1.5 --network localhost",
    "deploy:v1.5:localhost_l2": "npx hardhat deploy --tags V1.5 --network localhost_l2",
    "deploy:v1.5:base_sepolia": "npx hardhat deploy --tags V1.5 --network base_sepolia",
    "deploy:v1.5:sepolia": "npx hardhat deploy --tags V1.5 --network sepolia",
    "deploy:v1.5:holesky": "npx hardhat deploy --tags V1.5 --network holesky",
    "deploy:v1.5:mainnet": "npx hardhat deploy --tags V1.5 --network mainnet",
    "deploy:v1.5:base_mainnet": "npx hardhat deploy --tags V1.5 --network base_mainnet",
    "verify:v1.5:base_sepolia": "npx hardhat run ./src/verifyV1_5.ts --network base_sepolia",
    "verify:v1.5:sepolia": "npx hardhat run ./src/verifyV1_5.ts --network sepolia",
    "verify:v1.5:holesky": "npx hardhat run ./src/verifyV1_5.ts --network holesky",
    "verify:v1.5:mainnet": "npx hardhat run ./src/verifyV1_5.ts --network mainnet",
    "verify:v1.5:base_mainnet": "npx hardhat run ./src/verifyV1_5.ts --network base_mainnet",
    "test": "npx hardhat test",
    "format": "npx prettier --write --plugin=prettier-plugin-solidity contracts/**/*.sol deploy/**/*.ts src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeFiGeek-Community/bulk-auction-factory.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeFiGeek-Community/bulk-auction-factory/issues"
  },
  "homepage": "https://github.com/DeFiGeek-Community/bulk-auction-factory#readme",
  "devDependencies": {
    "@chainlink/contracts-ccip": "1.4.0",
    "@chainlink/local": "^0.0.1",
    "@nomicfoundation/hardhat-ethers": "^3.0.6",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.7",
    "@openzeppelin/contracts": "^4.9.3",
    "@types/prompts": "^2.4.9",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.5.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.4.2",
    "hardhat": "^2.14.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-deploy-ethers": "^0.4.2",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.1",
    "prettier-plugin-solidity": "^1.1.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
